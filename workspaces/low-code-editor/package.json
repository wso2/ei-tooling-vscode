{
  "name": "@wso2-ei/low-code-editor",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.23",
    "@types/react": "^18.0.33",
    "@types/react-dom": "^18.0.11",
    "@wso2-ei/low-code-diagram": "workspace:*",
    "@wso2-ei/syntax-tree": "workspace:*",
    "@material-ui/core": "^4.12.3",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.52",
    "@material-ui/pickers": "^3.2.10",
    "@sentry/browser": "^6.5.1",
    "@sentry/cli": "^1.71.0",
    "@sentry/react": "^6.16.1",
    "@sentry/tracing": "^6.16.1",
    "@sentry/webpack-plugin": "^1.18.8",
    "react-monaco-editor": "0.43.0",
    "react-intl": "^5.10.19",
    "react-lottie": "^1.2.3",
    "@types/react-lottie": "^1.2.5",
    "classnames": "^2.2.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "typescript": "^4.9.5",
    "copyfiles": "^2.4.1",
    "web-vitals": "^2.1.4",
    "@wso2-ei/low-code-editor-commons": "workspace:*",
    "vscode": "^1.1.29"
  },
  "scripts": {
    "prebuild": "pnpm run lint",
    "build": "pnpm run build:webpack && tsc --pretty && pnpm run copy:assets && pnpm run postbuild",
    "build:tsc": "tsc --pretty && pnpm run copy:assets",
    "build:webpack": "webpack --config webpack.config.js --mode=production",
    "copy:assets": "copyfiles -u 1 src/**/*.scss src/**/*.svg src/**/*.css src/**/*.png src/**/*.txt src/**/*.json build/esm",
    "watch": "pnpm run watch:webpack",
    "watch:tsc": "tsc --pretty --watch",
    "watch:webpack": "webpack-dev-server --mode=development --progress",
    "clean": "rimraf build/",
    "clean:nm": "rimraf node_modules",
    "postbuild": "copyfiles -f build/umd/SLCEditor.js ../vscode-plugin/resources/jslibs"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@wso2-ei/low-code-diagram": "workspace:*",
    "@wso2-ei/low-code-editor-commons": "workspace:*",
    "@types/testing-library__jest-dom": "^5.14.5",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "webpack": "^5.56.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.11.1",
    "style-loader": "^1.3.0",
    "css-loader": "^5.2.7",
    "source-map-loader": "^4.0.1",
    "ts-loader": "^9.4.1",
    "sass": "^1.56.1",
    "sass-loader": "^12.1.0",
    "vscode-languageserver-protocol": "^3.16.0",
    "vscode": "^1.1.29"
  }
}
